	public void evenArray() {
		
		for (int i = 0; i < ar1.length; i++) {
			//if (ar1[i] % 2 == 0) {
				System.out.print(ar1[i] + " ");
			//}
			
		}
		
	}
	
	public void setArray() {
		
		
		for (int i = 0; i < ar1.length; i++) {
			ar1[i] = i+1;
			System.out.print(ar1[i] + " ");
		}
		
